{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "readme": "README.md",
    "plugins": [
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/category"
    ],
    "source": {
        "include": [
            "src/"
        ],
        "includePattern": "\\.(js|ts)$"
    },
    "sourceType": "module",
    "opts": {
        "destination": "docs/",
        "readme": "readme.md",
        "template": "node_modules/better-docs"
    }
}